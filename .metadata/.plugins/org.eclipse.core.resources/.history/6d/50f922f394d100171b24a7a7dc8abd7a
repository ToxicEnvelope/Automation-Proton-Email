package com.proton.email.bot.core.tests;

import org.testng.Assert;
import org.testng.annotations.Test;

import com.proton.email.bot.core.pageobjects.LoginPage;

import net.thucydides.core.annotations.Step;

public class LoginTest extends ProtonTest {

	@FindBy(css="")
	private LoginPage login;
	
	@Step
	public void Create_an_account_with_free_plan_TEST() {
		login = new LoginPage(this.driver);
		Assert.assertTrue(login.attemptToClickOnSelectFreePlanBtn());
	}


}
