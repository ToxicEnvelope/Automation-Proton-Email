package com.proton.email.bot.core.utils;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Driver {

	
	public static WebDriver instance = null;
	public static String browser = "ch";
	
	public static void Initialize() {
		if(instance == null) {
			if(browser.equalsIgnoreCase("ch")) {
				
				String engine = "webdriver.chrome.driver", 
					   path = System.getProperty("user.dir") + "/Drivers/chromedriver";
				instance = new ChromeDriver();
			}
		}
		
		instance.manage().timeouts().implicitlyWait(, arg1)
	}
	
	
	
}
