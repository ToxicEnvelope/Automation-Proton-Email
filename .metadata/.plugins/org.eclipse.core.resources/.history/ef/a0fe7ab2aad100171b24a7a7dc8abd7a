package com.proton.email.bot.core.tests;

import com.proton.email.bot.core.pageobjects.FormPage;
import net.thucydides.core.annotations.Step;

public class FormTest extends ProtonTests {
	
	
	private FormPage form;
	
	@Step
	public void Fill_form_details_and_Create_Account_TEST1() {
		form = new FormPage(this.driver);
		form.attemptToFillFormDetails(getRandomString(), 0, pwd, email), selectIndex, pwd, email)
	}
	
	@Step
	public void Fill_form_details_and_Create_Account_TEST2() {
		form = new FormPage(this.driver);
		form.attemptToFillFormDetails(getRandomString(), 1, pwd, email), selectIndex, pwd, email)
	}
	
}
